generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model posts {
  id    Int    @id @default(autoincrement())
  title String @db.VarChar(2000)
  content String @db.VarChar(2000)
}

model users {
  id    Int    @id @default(autoincrement())
  email String @db.VarChar(255)
  password String @db.VarChar(255)
}
